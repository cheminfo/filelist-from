{
  "name": "filelist-from",
  "version": "0.0.0",
  "description": "Create a FileList from a path or a zip file",
  "main": "./lib/index.js",
  "module": "./lib-esm/index.js",
  "types": "./lib/index.d.ts",
  "browser": {
    "./lib-esm/fileListFromPath.js": "./lib-esm/fileListFromPath.browser.js"
  },
  "keywords": [],
  "author": "Luc Patiny",
  "license": "MIT",
  "files": [
    "src",
    "lib",
    "lib-esm"
  ],
  "scripts": {
    "check-types": "tsc --noEmit",
    "clean": "rimraf lib lib-esm",
    "eslint": "eslint src --cache",
    "eslint-fix": "npm run eslint -- --fix",
    "prepack": "npm run tsc",
    "prettier": "prettier --check src",
    "prettier-write": "prettier --write src",
    "test": "npm run test-only && npm run eslint && npm run prettier && npm run check-types",
    "test-only": "jest --coverage",
    "tsc": "npm run clean && npm run tsc-cjs && npm run tsc-esm",
    "tsc-cjs": "tsc --project tsconfig.cjs.json",
    "tsc-esm": "tsc --project tsconfig.esm.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cheminfo/filelist-from.git"
  },
  "bugs": {
    "url": "https://github.com/cheminfo/filelist-from/issues"
  },
  "homepage": "https://github.com/cheminfo/filelist-from#readme",
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "eslint": "^8.9.0",
    "eslint-config-cheminfo-typescript": "^10.3.0",
    "jest": "^27.5.1",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.3",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "jszip": "^3.7.1"
  }
}
